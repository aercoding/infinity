local Farm = Window:NewTab("Farm")
    local MainSection = Farm:NewSection("AUTO FARM")

    MainSection:NewToggle("Auto Sacrice", "Autofarm Sacrifice and Rarity TP", function(state)  
        if state then    
        _G.loop = true
        while _G.loop == true do wait()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/aercoding/infinity/main/sacrifice"))()
wait(0.2)
       end
        else
        _G.loop = false
        while _G.loop == true do wait()
        break
            end
        end
    end) 

 
MainSection:NewToggle("Auto Prestige", "Autofarm Prestige and Rarity TP", function(state)  
    if state then    
    _G.loop = true
    while _G.loop == true do wait()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/aercoding/infinity/main/prestige"))()
   end
    else
    _G.loop = false
    while _G.loop == true do wait()
    break
        end
    end
end)

MainSection:NewToggle("Auto Ascend Fast", "Autofarm Ascend and Rarity TP", function(state)  
    if state then    
    _G.loop = true
    while _G.loop == true do wait()
    local player = game.Players.LocalPlayer.Character
    local part = game.Workspace["RarityGet"] 
    player.HumanoidRootPart.CFrame = part.CFrame
    wait (1)
    local player = game.Players.LocalPlayer.Character
    local part = game.Workspace["Sacrifice"] 
    player.HumanoidRootPart.CFrame = part.CFrame
    wait (0.2)
    local player = game.Players.LocalPlayer.Character
    local part = game.Workspace["UpgradeLuck"] 
    player.HumanoidRootPart.CFrame = part.CFrame
    wait (0.3)
     local player = game.Players.LocalPlayer.Character
    local part = game.Workspace["RarityGet"] 
    player.HumanoidRootPart.CFrame = part.CFrame
    wait (1)

    local part = game.Workspace["Ascend"] 
    player.HumanoidRootPart.CFrame = part.CFrame
    wait (0.2)
   end
    else
    _G.loop = false
    while _G.loop == true do wait()
    break
        end
    end
end)
