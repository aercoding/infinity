local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Infinite Rarities By:AerPlays", "DarkTheme")

local startergui = game:GetService("StarterGui")
local player = game.Players.LocalPlayer.Character
local partR = game.Workspace["RarityGet"] 
local partS = game.Workspace["Sacrifice"]
local partP = game.Workspace["Prestige"]
local partA = game.Workspace["Ascend"]
    -- Main
    
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("TELEPORT")
 
    
    MainSection:NewButton("Prestige TP", "Makes you tp to prestige", function()
        local player = game.Players.LocalPlayer.Character
local part = game.Workspace["Prestige"] 
player.HumanoidRootPart.CFrame = part.CFrame
    end)
 
    MainSection:NewButton("Rarity Roll TP", "Makes you tp to RarityGet", function()
        local player = game.Players.LocalPlayer.Character
local part = game.Workspace["RarityGet"] 
player.HumanoidRootPart.CFrame = part.CFrame
    end)
    
